<?xml version="1.0" encoding="utf-8" ?>

<sqlMap namespace="MozartUI.Services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ibatis.apache.org/mapping">

	<resultMaps>
		<resultMap id="CfgSiteMaster-result"  class="CfgSiteMaster" >
			<result property="SiteId"    	column="SITE_ID"/>
			<result property="SiteType"    	column="SITE_TYPE"/>
			<result property="CreateTime"   column="CREATE_TIME"/>
			<result property="CreateUser"	column="CREATE_USER"/>
			<result property="UpdateTime"	column="UPDATE_TIME"/>
			<result property="UpdateUser"	column="UPDATE_USER"/>
		</resultMap>
	</resultMaps>

	<statements>

		<!--SELECT All-->
		<select id="CfgSiteMaster.SelectList" resultMap="CfgSiteMaster-result">
			SELECT [SITE_ID]
				  ,[SITE_TYPE]
				  ,[CREATE_TIME]
				  ,[CREATE_USER]
				  ,[UPDATE_TIME]
				  ,[UPDATE_USER]
			FROM [CFG_SITE_MASTER]
		</select>

		<!--SELECT BY ID-->
		<select id="CfgSiteMaster.Select" parameterClass="string" resultMap="CfgSiteMaster-result">
			SELECT [SITE_ID]
				  ,[SITE_TYPE]
				  ,[CREATE_TIME]
				  ,[CREATE_USER]
				  ,[UPDATE_TIME]
				  ,[UPDATE_USER]
			FROM [CFG_SITE_MASTER]
			WHERE
			[SITE_ID] = #SiteId#
		</select>

		<!--INSERT-->
		<insert id="CfgSiteMaster.Insert" parameterClass="CfgSiteMaster">
			INSERT INTO [CFG_SITE_MASTER]
				([SITE_ID]
				,[SITE_TYPE]
				,[CREATE_TIME]
				,[CREATE_USER]
				,[UPDATE_TIME]
				,[UPDATE_USER])
			VALUES
				(#SiteId#
				,#SiteType#
				,#CreateTime#
				,#CreateUser#
				,#CreateTime#
				,#CreateUser#)
		</insert>

		<!--UPDATE-->
		<update id="CfgSiteMaster.Update" parameterClass="CfgSiteMaster">
			UPDATE
				[CFG_SITE_MASTER]
			SET
			<dynamic prepend=" " close=" ">
				<isNotEmpty prepend="," property="SiteType">
					[SITE_TYPE] = #SiteType#
				</isNotEmpty>
				<isNotEmpty prepend="," property="UpdateTime">
					[UPDATE_TIME] = #UpdateTime#
				</isNotEmpty>
				<isNotEmpty prepend="," property="UpdateUser">
					[UPDATE_USER] = #UpdateUser#
				</isNotEmpty>
				WHERE
					[SITE_ID] = #SiteId#
			</dynamic>
		</update>

		<!--DELETE-->
		<delete id="CfgSiteMaster.Delete" parameterClass="string">
			DELETE
			FROM
				[CFG_SITE_MASTER]
			WHERE
				[SITE_ID] = #SiteId#
		</delete>

	</statements>
</sqlMap>