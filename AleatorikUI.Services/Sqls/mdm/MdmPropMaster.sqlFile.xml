<?xml version="1.0" encoding="utf-8" ?>

<sqlMap namespace="AleatorikUI.Services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ibatis.apache.org/mapping">

	<resultMaps>
		<resultMap id="rtnMdmPropMaster"  class="MdmPropMaster" >
			<result property="propertyID"   column="PROPERTY_ID"/>
			<result property="category"     column="CATEGORY"/>
			<result property="valueType"    column="VALUE_TYPE"/>
			<result property="description"  column="DESCRIPTION"/>
			<result property="reservedWord" column="RESERVED_WORD"/>
			<result property="defaultValue" column="DEFAULT_VALUE"/>
			<result property="isActive"     column="IS_ACTIVE"/>
			<result property="createTime"   column="CREATE_TIME"/>
			<result property="createUser"   column="CREATE_USER"/>
			<result property="updateTime"   column="UPDATE_TIME"/>
			<result property="updateUser"   column="UPDATE_USER"/>
		</resultMap>
	</resultMaps>
	
	<statements>
		<!--SELECT All-->
		<select id="MdmPropMaster.Select" resultMap="rtnMdmPropMaster">
			SELECT [PROPERTY_ID]
				 , [CATEGORY]
				 , [VALUE_TYPE]
				 , [DESCRIPTION]
				 , [RESERVED_WORD]
				 , [DEFAULT_VALUE]
				 , [IS_ACTIVE]
				 , [CREATE_TIME]
				 , [CREATE_USER]
				 , [UPDATE_TIME]
				 , [UPDATE_USER]
			  FROM [CFG_PROPERTY_MASTER]
		</select>

		<!--INSERT-->
		<insert id="MdmPropMaster.Insert" parameterClass="MdmPropMaster">
			INSERT INTO [CFG_PROPERTY_MASTER]
				 ( [PROPERTY_ID]
				 , [CATEGORY]
				 , [VALUE_TYPE]
				 , [DESCRIPTION]
				 , [RESERVED_WORD]
				 , [DEFAULT_VALUE]
				 , [IS_ACTIVE]
				 , [CREATE_TIME]
				 , [CREATE_USER]
				 , [UPDATE_TIME]
				 , [UPDATE_USER]
				 ) VALUES
				 ( #propertyID#
				 , #category#
				 , #valueType#
				 , #description#
				 , #reservedWord#
				 , #defaultValue#
				 , #isActive#
				 , #createTime#
				 , #createUser#
				 , #updateTime#
				 , #updateUser#
				 )
		</insert>

		<!--UPDATE-->
		<update id="MdmPropMaster.Update" parameterClass="MdmPropMaster">
			UPDATE [CFG_PROPERTY_MASTER]
			   SET [PROPERTY_ID]   = #propertyID#
				 , [CATEGORY]      = #category#
				 , [VALUE_TYPE]    = #valueType#
				 , [DESCRIPTION]   = #description#
				 , [RESERVED_WORD] = #reservedWord#
				 , [DEFAULT_VALUE] = #defaultValue#
				 , [IS_ACTIVE]     = #isActive#
				 , [CREATE_TIME]   = #createTime#
				 , [CREATE_USER]   = #createUser#
				 , [UPDATE_TIME]   = #updateTime#
				 , [UPDATE_USER]   = #updateUser#
			 WHERE [PROPERTY_ID]   = #propertyID#
		</update>

		<!--DELETE-->
		<delete id="MdmPropMaster.Delete" parameterClass="string">
			DELETE 
			  FROM [CFG_PROPERTY_MASTER]
			 WHERE [PROPERTY_ID]   = #propertyID#
		</delete>

	</statements>
</sqlMap>