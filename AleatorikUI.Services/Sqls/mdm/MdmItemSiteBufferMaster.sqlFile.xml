			  FROM [CFG_ITEM_SITE_BUFFER_MASTER]
			<dynamic prepend="WHERE " close=" ">
				<isNotEmpty prepend="AND " property="projectID">
					[PROJECT_ID]      = #projectID#
				</isNotEmpty>
				<isNotEmpty prepend="AND " property="itemID">
