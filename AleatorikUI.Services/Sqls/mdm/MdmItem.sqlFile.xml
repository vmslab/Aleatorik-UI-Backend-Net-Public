<?xml version="1.0" encoding="utf-8" ?>

<sqlMap namespace="AleatorikUI.Services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ibatis.apache.org/mapping">

	<resultMaps>
		<resultMap id="rtnMdmItem"  class="MdmItem" >
			<result property="itemID"     column="ITEM_ID"/>
			<result property="itemType"   column="ITEM_TYPE"/>
			<result property="createTime" column="CREATE_TIME"/>
			<result property="createUser" column="CREATE_USER"/>
			<result property="updateTime" column="UPDATE_TIME"/>
			<result property="updateUser" column="UPDATE_USER"/>
		</resultMap>
	</resultMaps>

	<statements>
		<!--SELECT All-->
		<select id="MdmItem.Select" resultMap="rtnMdmItem">
			SELECT [ITEM_ID]
				 , [ITEM_TYPE]
				 , [CREATE_TIME]
				 , [CREATE_USER]
				 , [UPDATE_TIME]
				 , [UPDATE_USER]
		  	  FROM [CFG_ITEM_MASTER]
		</select>

		<!--INSERT-->
		<insert id="MdmItem.Insert" parameterClass="MdmItem">
			INSERT INTO [CFG_ITEM_MASTER]
				 ( [ITEM_ID]
				 , [ITEM_TYPE]
				 , [CREATE_TIME]
				 , [CREATE_USER]
				 , [UPDATE_TIME]
				 , [UPDATE_USER]
				 ) VALUES
				 ( #itemID#
				 , #itemType#
				 , #createTime#
				 , #createUser#
				 , #updateTime#
				 , #updateUser#
				 )
		</insert>

		<!--UPDATE-->
		<update id="MdmItem.Update" parameterClass="MdmItem">
			UPDATE [CFG_ITEM_MASTER]
			   SET [ITEM_ID]     = #itemID#
				 , [ITEM_TYPE]   = #itemType#
				 , [CREATE_TIME] = #createTime#
				 , [CREATE_USER] = #createUser#
				 , [UPDATE_TIME] = #updateTime#
				 , [UPDATE_USER] = #updateUser#
		 	 WHERE [ITEM_ID]     = #itemID#
		</update>

		<!--DELETE-->
		<delete id="MdmItem.Delete" parameterClass="MdmItem">
			DELETE
			  FROM [CFG_ITEM_MASTER]
			 WHERE [ITEM_ID]     = #itemID#
		</delete>

	</statements>
</sqlMap>