<?xml version="1.0" encoding="utf-8" ?>

<sqlMap namespace="AleatorikUI.Services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ibatis.apache.org/mapping">

	<resultMaps>
		<resultMap id="rtnMdmIsb"  class="MdmIsb" >
			<result property="itemID"     column="ITEM_ID"/>
			<result property="siteID"     column="SITE_ID"/>
			<result property="bufferID"   column="BUFFER_ID"/>
			<result property="createTime" column="CREATE_TIME"/>
			<result property="createUser" column="CREATE_USER"/>
			<result property="updateTime" column="UPDATE_TIME"/>
			<result property="updateUser" column="UPDATE_USER"/>
		</resultMap>
	</resultMaps>

	<statements>
		<!--SELECT All-->
		<select id="MdmIsb.Select" resultMap="rtnMdmIsb">
			SELECT [ITEM_ID]
				 , [SITE_ID]
				 , [BUFFER_ID]
				 , [CREATE_TIME]
				 , [CREATE_USER]
				 , [UPDATE_TIME]
				 , [UPDATE_USER]
			 FROM [CFG_ITEM_SITE_BUFFER_MASTER]
		</select>

		<!--INSERT-->
		<insert id="MdmIsb.Insert" parameterClass="MdmIsb">
			INSERT INTO [CFG_ITEM_SITE_BUFFER_MASTER]
				 ( [ITEM_ID]
				 , [SITE_ID]
				 , [BUFFER_ID]
				 , [CREATE_TIME]
				 , [CREATE_USER]
				 , [UPDATE_TIME]
				 , [UPDATE_USER]
				 ) VALUES
				 ( #itemID#
				 , #siteID#
				 , #bufferID#
				 , #createTime#
				 , #createUser#
				 , #updateTime#
				 , #updateUser#
				 )
		</insert>

		<!--UPDATE-->
		<update id="MdmIsb.Update" parameterClass="MdmIsb">
			UPDATE [CFG_ITEM_SITE_BUFFER_MASTER]
			   SET [ITEM_ID]     = #itemID#
			     , [SITE_ID]     = #siteID#
			     , [BUFFER_ID]   = #bufferID#
			     , [CREATE_TIME] = #createTime#
			     , [CREATE_USER] = #createUser#
			     , [UPDATE_TIME] = #updateTime#
			     , [UPDATE_USER] = #updateUser#
			 WHERE [ITEM_ID]     = #itemID#
			   AND [SITE_ID]     = #siteID#
			   AND [BUFFER_ID]   = #bufferID#
		</update>

		<!--DELETE-->
		<delete id="MdmIsb.Delete" parameterClass="string">
			DELETE
			  FROM [CFG_ITEM_SITE_BUFFER_MASTER]
			 WHERE [ITEM_ID]     = #itemID#
			   AND [SITE_ID]     = #siteID#
			   AND [BUFFER_ID]   = #bufferID#
		</delete>

	</statements>
</sqlMap>