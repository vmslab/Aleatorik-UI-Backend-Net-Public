<?xml version="1.0" encoding="utf-8" ?>

<sqlMap namespace="AleatorikUI.Services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ibatis.apache.org/mapping">

	<resultMaps>
		<resultMap id="rtnMdmConstInfo"  class="MdmConstInfo" >
			<result property="constraintID" column="CONSTRAINT_ID"/>
			<result property="propertyID"   column="PROPERTY_ID"/>
			<result property="condition"    column="CONDITION"/>
			<result property="calendarID"   column="CALENDAR_ID"/>
			<result property="createTime"   column="CREATE_TIME"/>
			<result property="createUser"   column="CREATE_USER"/>
			<result property="updateTime"   column="UPDATE_TIME"/>
			<result property="updateUser"   column="UPDATE_USER"/>
		</resultMap>
	</resultMaps>

	<statements>
		<!--SELECT-->
		<select id="MdmConstInfo.Select" resultMap="rtnMdmConstInfo">
			SELECT [CONSTRAINT_ID]
				 , [PROPERTY_ID]
				 , [CONDITION]
				 , [CALENDAR_ID]
				 , [CREATE_TIME]
				 , [CREATE_USER]
				 , [UPDATE_TIME]
				 , [UPDATE_USER]
			  FROM [CFG_CONSTRAINT_INFO]
		</select>



		<!--INSERT-->
		<insert id="MdmConstInfo.Insert" parameterClass="MdmConstInfo">
			INSERT INTO [CFG_CONSTRAINT_INFO]
				 ( [CONSTRAINT_ID]
				 , [PROPERTY_ID]
				 , [CONDITION]
				 , [CALENDAR_ID]
				 , [CREATE_TIME]
				 , [CREATE_USER]
				 , [UPDATE_TIME]
				 , [UPDATE_USER]
				 ) VALUES
				 ( #constraintID#
				 , #propertyID#
				 , #condition#
				 , #calendarID#
				 , getdate()
				 , #createUser#
				 , getdate()
				 , #updateUser#
				 )
		</insert>

		<!--UPDATE-->
		<update id="MdmConstInfo.Update" parameterClass="MdmConstInfo">
			UPDATE [CFG_CONSTRAINT_INFO]
			   SET [CONSTRAINT_ID] = #constraintID#
				 , [PROPERTY_ID]   = #propertyID#
				 , [CONDITION]     = #condition#
				 , [CALENDAR_ID]   = #calendarID#
				 , [UPDATE_TIME]   = getdate()
				 , [UPDATE_USER]   = #updateUser#
			 WHERE [CONSTRAINT_ID] = #constraintID#
		</update>

		<!--DELETE-->
		<delete id="MdmConstInfo.Delete" parameterClass="string">
			DELETE
			  FROM [CFG_CONSTRAINT_INFO]
			 WHERE [CONSTRAINT_ID] = #constraintID#
		</delete>

	</statements>
</sqlMap>