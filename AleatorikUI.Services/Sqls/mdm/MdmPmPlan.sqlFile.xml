<?xml version="1.0" encoding="utf-8" ?>

<sqlMap namespace="AleatorikUI.Services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ibatis.apache.org/mapping">

	<resultMaps>
		<resultMap id="rtnMdmPmPlan"  class="MdmPmPlan" >
			<result property="pmID"         column="PM_ID"/>
			<result property="priority"     column="PRIORITY"/>
			<result property="startTime"    column="START_TIME"/>
			<result property="endTime"      column="END_TIME"/>
			<result property="pmTime"       column="PM_TIME"/>
			<result property="patternName"  column="PATTERN_NAME"/>
			<result property="patternValue" column="PATTERN_VALUE"/>
			<result property="pmPolicy"     column="PM_POLICY"/>
			<result property="parameter"    column="PARAMETER"/>
			<result property="createTime"   column="CREATE_TIME"/>
			<result property="createUser"   column="CREATE_USER"/>
			<result property="updateTime"   column="UPDATE_TIME"/>
			<result property="updateUser"   column="UPDATE_USER"/>
		</resultMap>
	</resultMaps>

	<statements>
		<!--SELECT All-->
		<select id="MdmPmPlan.Select" resultMap="rtnMdmPmPlan">
			SELECT [PM_ID]
				 , [PRIORITY]
				 , [START_TIME]
				 , [END_TIME]
				 , [PM_TIME]
				 , [PATTERN_NAME]
				 , [PATTERN_VALUE]
				 , [PM_POLICY]
				 , [PARAMETER]
				 , [CREATE_TIME]
				 , [CREATE_USER]
				 , [UPDATE_TIME]
				 , [UPDATE_USER]
			  FROM [CFG_PM_PLAN]
		</select>

		<!--INSERT-->
		<insert id="MdmPmPlan.Insert" parameterClass="MdmPmPlan">
			INSERT INTO [CFG_PM_PLAN]
				 ( [PM_ID]
				 , [PRIORITY]
				 , [START_TIME]
				 , [END_TIME]
				 , [PM_TIME]
				 , [PATTERN_NAME]
				 , [PATTERN_VALUE]
				 , [PM_POLICY]
				 , [PARAMETER]
				 , [CREATE_TIME]
				 , [CREATE_USER]
				 , [UPDATE_TIME]
				 , [UPDATE_USER]
				 ) VALUES
				 ( #pmID#
				 , #priority#
				 , #startTime#
				 , #endTime#
				 , #pmTime#
				 , #patternName#
				 , #patternValue#
				 , #pmPolicy#
				 , #parameter#
				 , getdate()
				 , #createUser#
				 , getdate()
				 , #updateUser#
				 )
		</insert>

		<!--UPDATE-->
		<update id="MdmPmPlan.Update" parameterClass="MdmPmPlan">
			UPDATE [CFG_PM_PLAN]
			   SET [PM_ID]         = #pmID#
				 , [PRIORITY]      = #priority#
				 , [START_TIME]    = #startTime#
				 , [END_TIME]      = #endTime#
				 , [PM_TIME]       = #pmTime#
				 , [PATTERN_NAME]  = #patternName#
				 , [PATTERN_VALUE] = #patternValue#
				 , [PM_POLICY]     = #pmPolicy#
				 , [PARAMETER]     = #parameter#
				 , [UPDATE_TIME]   = getdate()
				 , [UPDATE_USER]   = #updateUser#
			 WHERE PM_ID    = #pmID#
			   AND PRIORITY = #priority#
		</update>

		<!--DELETE-->
		<delete id="MdmPmPlan.Delete" parameterClass="string">
			DELETE
			  FROM [CFG_PM_PLAN]
			 WHERE PM_ID    = #pmID#
			   AND PRIORITY = #priority#
		</delete>

	</statements>
</sqlMap>