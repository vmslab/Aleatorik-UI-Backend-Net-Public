				<isNotEmpty prepend="AND " property="patternValue">
					[PATTERN_VALUE]    = #patternValue#
				</isNotEmpty>
				<isNotEmpty prepend="AND " property="patternPriority">
					[PATTERN_PRIORITY] = #patternPriority#
				</isNotEmpty>
				<isNotEmpty prepend="AND " property="description">
					[DESCRIPTION]      = #description#
				</isNotEmpty>
				<isNotEmpty prepend="AND " property="createUser">
					[CREATE_USER]      = #createUser#
				</isNotEmpty>
				<isNotEmpty prepend="AND " property="updateUser">
					[UPDATE_USER]      = #updateUser#
				</isNotEmpty>
			</dynamic>
		</select>

		<!--INSERT-->
		<insert id="MdmCalendarDetail.Insert" parameterClass="MdmCalendarDetail">
			INSERT INTO [CFG_CALENDAR_DETAIL]
				 ( [PROJECT_ID]
				 , [CALENDAR_ID]
				 , [PATTERN_ID]
				 , [EFF_START_DATE]
				 , [EFF_END_DATE]
				 , [PATTERN_TYPE]
				 , [PATTERN_VALUE]
				 , [PATTERN_PRIORITY]
				 , [DESCRIPTION]
				 , [CREATE_DATETIME]
				 , [CREATE_USER]
				 , [UPDATE_DATETIME]
				 , [UPDATE_USER]
