				 , [PRIORITY]
				 , [EFF_START_TIME]
				 , [EFF_END_TIME]
				 , [CREATE_TIME]
				 , [CREATE_USER]
				 , [UPDATE_TIME]
				 , [UPDATE_USER]
				 ) VALUES
