				 ( [PROJECT_ID]
				 , [BOM_ID]
				 , [BOM_TYPE]
				 , [BOM_PRIORITY]
				 , [EFF_START_DATE]
				 , [EFF_END_DATE]
				 , [DESCRIPTION]
				 , [CREATE_DATETIME]
				 , [CREATE_USER]
				 , [UPDATE_DATETIME]
				 , [UPDATE_USER]
				 ) VALUES
				 ( #projectID#
				 , #bomID#
				 , #bomType#
				 , #bomPriority#
				 , #effStartDate#
				 , #effEndDate#
				 , #description#
				 , getdate()
				 , #createUser#
				 , getdate()
				 , #updateUser#
				 )
		</insert>

