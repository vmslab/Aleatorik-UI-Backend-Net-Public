				 , [BOM_TYPE]
				 , [BOM_PRIORITY]
				 , [EFF_START_DATE]
				 , [EFF_END_DATE]
				 , [DESCRIPTION]
				 , [CREATE_DATETIME]
				 , [CREATE_USER]
				 , [UPDATE_DATETIME]
				 , [UPDATE_USER]
			  FROM [CFG_BOM_MASTER]
			 WHERE [PROJECT_ID] = #projectID#
		</select>

		<!--INSERT-->
		<insert id="MdmBomMaster.Insert" parameterClass="MdmBomMaster">
